{
  "name": "ley-delivery-api",
  "module": "index.ts",
  "type": "module",
  "scripts": {
    "dev": "bun --watch src/http/server.ts",
    "start": "NODE_ENV=production bun src/http/server.ts",
    "build": "bun build src/http/server.ts ",
    "test": "bun run vitest --dir src/use-cases",
    "test:watch": "bun run vitest --watch src/use-cases",
    "test:coverage": "bun run vitest --coverage src/use-cases",
    "lint": "eslint --fix src --ext .ts,.tsx",
    "generate": "drizzle-kit generate:pg",
    "migrate": "bun src/database/migrate.ts",
    "seed": "bun src/database/seed.ts",
    "studio": "bun drizzle-kit studio"
  },
  "dependencies": {
    "@paralleldrive/cuid2": "^2.2.2",
    "bcrypt": "^5.1.1",
    "drizzle-orm": "^0.30.7",
    "fastify": "^4.26.2",
    "postgres": "^3.4.4",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@rocketseat/eslint-config": "^2.2.2",
    "@types/bcrypt": "^5.0.2",
    "@types/bun": "latest",
    "@vitest/coverage-v8": "^1.4.0",
    "chalk": "^5.3.0",
    "drizzle-kit": "^0.20.14",
    "eslint": "^8.57.0",
    "eslint-plugin-drizzle": "^0.2.3",
    "pg": "^8.11.5",
    "tsc": "^2.0.4",
    "typescript": ">=5.0.0",
    "vitest": "^1.4.0"
  }
}
