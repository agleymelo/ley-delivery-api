{
  "name": "ley-delivery-api",
  "module": "index.ts",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/http/server.ts",
    "start": "NODE_ENV=production node build/http/server.ts",
    "build": "tsup src --out-dir build",
    "test": "vitest --dir src/use-cases",
    "test:watch": "vitest --watch src/use-cases",
    "test:coverage": "vitest --coverage src/use-cases",
    "lint": "eslint --fix src --ext .ts,.tsx",
    "generate": "drizzle-kit generate:pg",
    "migrate": "src/database/migrate.ts",
    "seed": "tsx src/database/seed.ts",
    "studio": "drizzle-kit studio"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.552.0",
    "@aws-sdk/lib-storage": "^3.552.0",
    "@fastify/cors": "^9.0.1",
    "@fastify/jwt": "^8.0.0",
    "@fastify/static": "7.0.3",
    "@paralleldrive/cuid2": "^2.2.2",
    "bcrypt": "^5.1.1",
    "dayjs": "1.11.10",
    "dotenv": "16.4.5",
    "drizzle-orm": "^0.30.7",
    "fastify": "^4.26.2",
    "fastify-multer": "2.0.3",
    "multer": "^1.4.5-lts.1",
    "postgres": "^3.4.4",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@rocketseat/eslint-config": "^2.2.2",
    "@types/bcrypt": "^5.0.2",
    "@types/bun": "latest",
    "@vitest/coverage-v8": "^1.4.0",
    "chalk": "^5.3.0",
    "drizzle-kit": "^0.20.14",
    "eslint": "^8.57.0",
    "eslint-plugin-drizzle": "^0.2.3",
    "pg": "^8.11.5",
    "tsc": "^2.0.4",
    "tsup": "8.0.2",
    "tsx": "^4.7.2",
    "typescript": ">=5.0.0",
    "vitest": "^1.4.0"
  },
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:agleymelo/ley-delivery-api.git",
  "author": "Agleylson Melo <agleylsonmelo@outlook.com>",
  "license": "MIT"
}
